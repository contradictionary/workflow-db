{
  "name": "workflow-db",
  "version": "1.0.0",
  "description": "To store the workflow db",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "init-mysql": "npx prisma init --datasource-provider mysql --schema=./schema/database.mysql.prisma",
    "generate-mysql": "npx prisma generate --schema=./schema/database.mysql.prism",
    "migrate-prod-mysql": "npx prisma migrate prod --name initial --schema=./schema/database.mysql.prism",
    "migrate-dev-mysql": "npx prisma migrate dev --name initial --schema=./schema/database.mysql.prism",
    "init-sqlite": "npx prisma init --datasource-provider sqlite --schema=./schema/database.sqlite.prisma",
    "generate-sqlite": "npx prisma generate --schema=./schema/database.sqlite.prism",
    "migrate-prod-sqlite": "npx prisma migrate prod --name initial --schema=./schema/database.sqlite.prism",
    "migrate-dev-sqlite": "npx prisma migrate dev --name initial --schema=./schema/database.sqlite.prism"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contradictionary/workflow-db.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/contradictionary/workflow-db/issues"
  },
  "homepage": "https://github.com/contradictionary/workflow-db#readme",
  "devDependencies": {
    "bcrypt": "^5.1.0",
    "prisma": "^4.14.1"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1"
  }
}