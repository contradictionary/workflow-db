{
  "name": "workflow-db",
  "version": "1.0.0",
  "description": "to store the workflow db",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "init": "npx prisma init --datasource-provider mysql",
    "generate": "npx prisma generate --schema=./schema/database.prisma",
    "migrate-prod": "npx prisma migrate prod --name initial --schema=./schema/database.prisma",
    "migrate-dev": "npx prisma migrate dev --name initial --schema=./schema/database.prisma",
    "seed": "node ./seed/seed-initial.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contradictionary/workflow-db.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/contradictionary/workflow-db/issues"
  },
  "homepage": "https://github.com/contradictionary/workflow-db#readme",
  "devDependencies": {
    "bcrypt": "^5.1.0",
    "prisma": "^4.14.1"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1"
  }
}